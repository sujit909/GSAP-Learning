    "@babel/register": "^7.13.16",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.14",
    "browserslist": "^4.16.6",
    "clean-html": "^1.5.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.5",
    "del": "^6.0.0",
    "dynamic-webpack-entries": "^1.0.1",
